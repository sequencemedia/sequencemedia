{
  "extends": "standard",
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "overrides": [
    {
      "files": [
        "babel.config.js"
      ],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
