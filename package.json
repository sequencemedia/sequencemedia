{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.0.300",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run build:js\" \"npm run build:css\"",
    "build:clean": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:clean\" \"npm run build:css:clean\"",
    "build:css": "npm run gulp -- build:css",
    "build:css:clean": "npm run gulp -- build:css:clean",
    "build:css:watch": "npm run gulp -- build:css:watch",
    "build:js": "npm run webpack",
    "build:js:clean": "rimraf pub/assets/js/*.js pub/assets/js/*.js.map",
    "build:js:watch": "npm run webpack -- -w",
    "build:watch": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:watch\" \"npm run build:css:watch\"",
    "gulp": "gulp --color",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint . --ext .mjs,.cjs,.js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "nvm": "bash nvm.sh",
    "pregulp": "npm run nvm",
    "prepare": "husky",
    "prewebpack": "npm run nvm",
    "webpack": "webpack"
  },
  "dependencies": {
    "debug": "^4.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@sequencemedia/gulp": "^1.1.190",
    "@sequencemedia/gulp-clean-css": "^1.0.105",
    "@sequencemedia/gulp-css-purge": "^1.1.106",
    "@sequencemedia/gulp-debug": "^1.0.108",
    "@sequencemedia/gulp-postcss": "^1.0.119",
    "@sequencemedia/gulp-sass": "^1.0.104",
    "@sequencemedia/gulp-sourcemaps": "^1.0.109",
    "@sequencemedia/hooks": "^1.0.495",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.2",
    "del": "^7.1.0",
    "eslint": "8.57.0",
    "eslint-config-standard": "^17.1.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.3.1",
    "terser-webpack-plugin": "^5.3.10",
    "vinyl-paths": "^5.0.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "imports": {
    "#build/gulp": "./build/gulp/index.mjs",
    "#build/gulp/css-from-sass": "./build/gulp/css-from-sass.mjs",
    "#build/gulp/handle-watch-error": "./build/gulp/handle-watch-error.mjs"
  }
}
