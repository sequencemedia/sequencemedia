{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.1.140",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "https://sequencemedia.net"
  },
  "scripts": {
    "build": "DEBUG=sequencemedia* npm run webpack",
    "build:clean": "rimraf pub/assets/css/*.css pub/assets/css/*.css.map pub/assets/js/*.cjs pub/assets/js/*.mjs pub/assets/js/*.cjs.map pub/assets/js/*.mjs.map 2> /dev/null",
    "build:watch": "npm run webpack -- -w",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "nvm": "bash nvm.sh",
    "prepare": "husky",
    "prettier": "prettier src/sass/**/*.scss --write",
    "prewebpack": "npm run nvm",
    "transform": "DEBUG=sequencemedia* ./transform.sh",
    "webpack": "webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@sequencemedia/eslint-config-standard": "^0.2.79",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "core-js": "^3.45.1",
    "css-loader": "^7.1.2",
    "debug": "^4.4.1",
    "eslint": "^9.34.0",
    "glob-all": "^3.3.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.4",
    "perfectionist": "^2.4.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "rimraf": "^6.0.1",
    "sass": "^1.92.0",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  }
}
