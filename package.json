{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.0.1",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "author": "Jonathan Perry for Sequence Media Limited <sequencemedia@sequencemedia.net>",
  "scripts": {
    "build:clean": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:clean\" \"npm run build:css:clean\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run build:js\" \"npm run build:css\"",
    "build:watch": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:watch\" \"npm run build:css:watch\"",
    "build:js:clean": "rimraf pub/assets/js/*.js pub/assets/js/*.js.map",
    "build:js": "webpack",
    "build:js:watch": "npm run build:js -- -w",
    "build:css:clean": "gulp build:css:clean",
    "build:css": "gulp build:css",
    "build:css:watch": "gulp build:css:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "pre-commit": "npm run lint && npm run build && git add pub"
  },
  "dependencies": {
    "debug": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^15.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.3.0",
    "module-alias": "^2.2.2",
    "postcss": "^8.1.4",
    "postcss-scss": "^3.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^5.0.2",
    "vinyl-paths": "^3.0.1",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
