{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.0.56",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "type": "module",
  "author": "Jonathan Perry for Sequence Media Limited <sequencemedia@sequencemedia.net>",
  "scripts": {
    "build:clean": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:clean\" \"npm run build:css:clean\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run build:js\" \"npm run build:css\"",
    "build:watch": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:watch\" \"npm run build:css:watch\"",
    "build:js:clean": "rimraf pub/assets/js/*.js pub/assets/js/*.js.map",
    "build:js": "npm run webpack",
    "build:js:watch": "npm run webpack -- -w",
    "build:css:clean": "npm run gulp -- build:css:clean",
    "build:css": "npm run gulp -- build:css",
    "build:css:watch": "npm run gulp -- build:css:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint . --ext .mjs --ext .cjs --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "nvm": "bash nvm.sh",
    "prewebpack": "npm run nvm",
    "webpack": "webpack",
    "pregulp": "npm run nvm",
    "gulp": "gulp --color",
    "prepare": "husky install"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "@sequencemedia/gulp": "^1.1.8",
    "@sequencemedia/gulp-clean-css": "^1.0.2",
    "@sequencemedia/gulp-css-purge": "^1.0.3",
    "@sequencemedia/gulp-debug": "^1.0.5",
    "@sequencemedia/gulp-postcss": "^1.0.2",
    "@sequencemedia/gulp-sass": "^1.0.1",
    "@sequencemedia/gulp-sourcemaps": "^1.0.4",
    "@sequencemedia/hooks": "^1.0.432",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.1.0",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "postcss-scss": "^4.0.6",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "stylelint": "^15.6.3",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "^5.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "vinyl-paths": "3.0.1",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "imports": {
    "#build/gulp": "./build/gulp/index.mjs",
    "#build/gulp/css-from-sass": "./build/gulp/css-from-sass.mjs",
    "#build/gulp/handle-watch-error": "./build/gulp/handle-watch-error.mjs"
  }
}
