{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.1.48",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "https://sequencemedia.net"
  },
  "scripts": {
    "build": "npm run webpack",
    "build:clean": "rimraf pub/assets/css/*.css pub/assets/css/*.css.map pub/assets/js/*.cjs pub/assets/js/*.mjs pub/assets/js/*.cjs.map pub/assets/js/*.mjs.map 2> /dev/null",
    "build:watch": "npm run webpack -- -w",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "nvm": "bash nvm.sh",
    "prepare": "husky",
    "prettier": "prettier src/**/*.scss --write",
    "prewebpack": "npm run nvm",
    "webpack": "webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@sequencemedia/eslint-config-standard": "^0.2.28",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.40.0",
    "css-loader": "^7.1.2",
    "debug": "^4.4.0",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.2",
    "perfectionist": "^2.4.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "rimraf": "^6.0.1",
    "sass": "^1.85.0",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.11.1",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
