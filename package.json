{
  "name": "@sequencemedia/sequencemedia",
  "version": "1.0.3",
  "description": "@SequenceMedia/sequencemedia",
  "private": true,
  "type": "module",
  "author": "Jonathan Perry for Sequence Media Limited <sequencemedia@sequencemedia.net>",
  "scripts": {
    "build:clean": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:clean\" \"npm run build:css:clean\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run build:js\" \"npm run build:css\"",
    "build:watch": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:watch\" \"npm run build:css:watch\"",
    "build:js:clean": "rimraf pub/assets/js/*.js pub/assets/js/*.js.map",
    "build:js": "source nvm.sh && webpack",
    "build:js:watch": "npm run build:js -- -w",
    "build:css:clean": "gulp build:css:clean",
    "build:css": "source nvm.sh && gulp build:css",
    "build:css:watch": "gulp build:css:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint . --ext .mjs --ext .cjs --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "nvm": "bash nvm.sh",
    "pre-commit": "npm run lint && npm run build && git add pub",
    "prepare": "husky install"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "core-js": "^3.29.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.15",
    "del": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "module-alias": "^2.2.2",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "rimraf": "^4.4.1",
    "sass": "^1.59.3",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-scss": "^4.5.0",
    "terser-webpack-plugin": "^5.3.7",
    "vinyl-paths": "3.0.1",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "imports": {
    "#build/gulp": "./build/gulp/index.mjs",
    "#build/gulp/css-from-sass": "./build/gulp/css-from-sass.mjs",
    "#build/gulp/handle-watch-error": "./build/gulp/handle-watch-error.mjs"
  }
}
